{
    "dataRoles": [
        {
            "displayName":"Road Number",
            "name":"road_number",
            "description":"The road number identifying the state or local government road",
            "kind":"Grouping",
            "preferredTypes":[
                {"text":true}
            ]
        },
        {
            "displayName":"SLK From",
            "name":"slk_from",
            "description":"The linear measurement along the Road at which this row of data starts",
            "kind":"Grouping",
            "requiredTypes":[
                {"numeric":true}
            ]
        },
        {
            "displayName":"SLK To",
            "name":"slk_to",
            "description":"The linear measurement along the Road at which this row of data ends",
            "kind":"Grouping",
            "requiredTypes":[
                {"numeric":true}
            ]
        },
        {
            "displayName":"Carriageway (L|R|S|LS|RS|LRS)",
            "name":"cwy",
            "description":"A text value consisting of any combination of the letters `L`, `R` and `S` identifying the Left, Right or Single carriageway. If left blank will default to `LRS`",
            "kind":"Grouping",
            "preferredTypes":[
                {"text":true}
            ]
        },
        {
            "displayName":"Offset Metres (+ or -)",
            "name":"offset",
            "description":"The distance in meters to offset the line segment. Values can be negative or positive: Positive will offset to the right hand side from the perspective of a vehicle travelling in the direction of increasing slk. Keep as small as possible to avoid distortion and invisible features.",
            "kind":"Grouping",
            "preferredTypes":[
                {"numeric":true}
            ]
        },
        {
            "displayName":"Colour (CSS)",
            "name":"colour",
            "description":"The colour to give each line. Must be a valid CSS colour value. Google 'CSS Colour' and you will find nice charts. Support for PowerBI's automated colour systems in the future.",
            "kind":"Grouping",
            "preferredTypes":[
                {"text":true}
            ]
        },
        {
            "displayName": "Tooltips",
            "name": "tooltips",
            "kind": "Grouping"
        }
    ],
    "objects": {
        "line_format": {
            "properties": {
                "default_line_colour": {"type": {"fill": {"solid": {"color":true}}}},
                "default_line_width" : {"type": {"numeric": true}}
            }
        },
        "road_network": {
            "properties": {
                "show"             : {"type": {"bool": true}},
                "show_ticks"       : {"type": {"bool": true}},
                "state_road_colour": {"type": {"fill": {"solid": {"color":true}}}},
                "psp_colour"       : {"type": {"fill": {"solid": {"color":true}}}}
            }
        },
        "map_background": {
            "properties": {
                "url_wmts"             : {"type": {"text":true}},
                "url_wmts_show"        : {"type": {"bool":true}},
                "url_tile_arcgis"      : {"type": {"text":true}},
                "url_tile_arcgis_show" : {"type": {"bool":true}},

                "osm_brightness"       :{"type": {"numeric":true}},
                "osm_greyscale"        :{"type": {"numeric":true}}
            }
        },
        "map_behaviour": {
            "properties": {
                "auto_zoom":{"type": {"bool":true}},
                "controls_size":{"type": {"numeric":true}},
                "controls_mode":{"type": {"enumeration": [
                    {"displayName": "Disabled",  "value": "Disabled"},
                    {"displayName": "Collapsed", "value": "Collapsed"},
                    {"displayName": "Expanded",  "value": "Expanded"}
                ]}}
            }
        },
        "advanced_settings":{
            "properties":{
                "offset_multiplier"  : {"type": {"numeric": true}},
                "allow_drag_box_selection": {"type":{"bool": true}}
            }
        }
    },
    "dataViewMappings": [
        {
            "table": {
                
                "rows": {
                    "dataReductionAlgorithm": {
                        "top": {"count": 30000}
                    },
                    "select": [
                        {"bind": {"to": "road_number"}},
                        {"bind": {"to": "cwy"}},
                        {"bind": {"to": "slk_from"}},
                        {"bind": {"to": "slk_to"}},
                        {"bind": {"to": "offset"}},
                        {"bind": {"to": "colour"}},
                        {"bind": {"to": "tooltips"}}
                    ]
                }
            }
        }
    ],
    "privileges": [
        {
            "name": "WebAccess",
            "essential": true,
            "parameters": [
                "*"
            ]
        }
    ],
    "suppressDefaultTitle": true,
    "tooltips": {
        "supportedTypes": {
            "default": true,
            "canvas": true
        },
        "roles": [
            "tooltips"
        ]
    }
}